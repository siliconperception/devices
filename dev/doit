#!/bin/bash
python pretrain.py --dataset tiny --shuffle --learning_rate 0.0001 --schedule linear --start_factor 0.1 --period 350 --opt adamw --batch 50 --bos 2 --monitor 10 --generate 100 --save 1000 --alt lite-base --slow 0.000152415
python pretrain.py --dataset tiny --shuffle --learning_rate 0.00001 --opt adam --batch 50 --bos 2 --monitor 10 --generate 100 --save 1000 --alt lite-base --slow 0.000152415 --load checkpoint.pt
python generate.py --device cpu --load checkpoint.pt --alt lite-base --vis
